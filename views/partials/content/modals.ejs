<!-- Portfolio Modals -->
<section id="portfolio">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2>Portfolio</h2>
                <hr class="star-primary">
            </div><!--<col-lg-12 text-center>-->
        </div><!--<row>-->
        <div class="row" itemscope itemtype="<%= itemtype %>Organization">
            <% if (images.length > 0) { %>
                <% for(var i = 0; i < images.length; i++) { %>
                    <div class="col-sm-4 portfolio-item">
                        <a href="#<%= portf + i %>" class="portfolio-link" data-toggle="modal">
                            <div class="caption">
                                <div class="caption-content">
                                    <i class="fa fa-search-plus fa-3x"></i>
                                </div><!--<caption-content>-->
                            </div><!--<caption>-->
                            <img src="./img/portfolio/<%= images[i] %>" class="img-responsive" alt="<%= alt[i] %>" itemprop="logo">
                        </a>
                    </div><!--col-sm-4 portfolio-item-->
                <% } %>
            <% } %>
        </div><!--<row>-->    
    </div><!--<container>-->
</section>